{
  "name": "main-angular-mdbootstrap-aot-ssr.modele",
  "version": "1.0.0",
  "description": "Angular - MDBootstrap - WebPack - AOT - JIT - SSR",
  "repository": {},
  "scripts": {
    "clean": "rimraf aot && rimraf build && rimraf dist",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "tsc:client": "tsc -p src/tsconfig.json",
    "tsc:server": "tsc -p src/tsconfig.server.json",
    "tsc": "rimraf build && npm run tsc:client && npm run tsc:server",
    "ngc:client": "ngc -p tsconfig-aot.json",
    "ngc:server": "ngc -p tsconfig.server-aot.json",
    "ngc": "rimraf aot && npm run ngc:client && npm run ngc:server",
    "test": "karma start karma.conf.js",
    "start": "webpack-dev-server --config config/webpack.client.dev.js --inline --progress",
    "build:client": "webpack --config config/webpack.client.prod.js -p --progress --profile --bail",
    "build:server": "webpack --config config/webpack.server.prod.js --progress --profile --bail",
    "build": "rimraf dist/jit && npm run build:client && npm run build:server",
    "build:client:aot": "npm run ngc:client && webpack --config config/webpack.client.prod.aot.js -p --progress --profile --bail",
    "build:server:aot": "npm run ngc:server && webpack --config config/webpack.server.prod.aot.js -p --progress --profile --bail",
    "build:aot": "rimraf dist/aot && npm run build:client:aot && npm run build:server:aot",
    "server": "node dist/jit/server.js",
    "server:aot": "node dist/aot/server.js"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.3.1",
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/platform-server": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@nguniversal/express-engine": "^1.0.0-beta.2",
    "angular2-highcharts": "^0.5.5",
    "angular2-markdown": "^1.6.0",
    "core-js": "^2.4.1",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "marked": "^0.3.6",
    "rxjs": "^5.4.2",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.3.1",
    "@ngtools/webpack": "^1.5.4",
    "@types/express": "^4.0.36",
    "@types/marked": "0.0.28",
    "@types/node": "^8.0.17",
    "@types/underscore": "^1.8.1",
    "compression": "^1.7.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "style-loader": "^0.18.2",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "underscore": "^1.8.3",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.11.0",
    "npm": ">= 5.0.3"
  }
}
